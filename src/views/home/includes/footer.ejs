  <!-- codigo scrollbar-->
  <script type="text/javascript">

      var myScroll;
      
      async function loaded(altura_) {

          return new Promise( async (resolve, reject) => {

              if(myScroll){
                  myScroll.destroy();
              }
              myScroll = null;
              myScroll = new IScroll('#wrapper', {
                  useTransition: true,
                bottomOffset: altura_,
                  scrollbars: true,
                  mouseWheel: true,
                  interactiveScrollbars: true,
                  shrinkScrollbars: 'scale',
                  fadeScrollbars: true
              });
            
              
              if(myScroll){
                  resolve(true);
              }



              /* rolar pagina ao bottom */
              //$( "#wrapper" ).scrollTop(altura_);
              //$("#scroller").animate({ scrollTop: altura_}, 1000);
              var scroll=$('#wrapper');
              scroll.animate({scrollTop: altura_});

              /* setTimeout(function() {
                  $('#scroller').css({'transform' : 'translate(0px, ' + altura_ + 'px)'});
              }, 1000); */

          });
      }

      document.addEventListener('touchmove', function (e) { e.preventDefault(); }, false);
      
